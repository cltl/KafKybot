            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/2/merged.owl#warm -->

    <owl:Class rdf:about="&merged;warm">
        <rdfs:subClassOf rdf:resource="&merged;temperature_region"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/2/merged.owl#weight_region -->

    <owl:Class rdf:about="&merged;weight_region">
        <rdfs:subClassOf rdf:resource="&DomainKyoto2;magnitude_region"/>
