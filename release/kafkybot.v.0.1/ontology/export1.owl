    <owl:ObjectProperty rdf:about="&SpatialRelations;spatial-location-of">
        <rdfs:range rdf:resource="&DOLCE-Lite;physical-endurant"/>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;physical-location-of"/>
