    <!-- http://www.semanticweb.org/ontologies/2010/2/merged.owl#vital_status_region -->

    <owl:Class rdf:about="&merged;vital_status_region">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&DomainKyoto2;organism_quality_region"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&Kyoto;dead"/>
