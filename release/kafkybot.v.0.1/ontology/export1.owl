        <rdfs:range rdf:resource="&DOLCE-Lite;perdurant"/>
        <owl:inverseOf rdf:resource="&TemporalRelations;follows"/>
        <rdfs:subPropertyOf rdf:resource="&TemporalRelations;temporal-relation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/TemporalRelations.owl#present-at -->

    <owl:ObjectProperty rdf:about="&TemporalRelations;present-at">
        <rdfs:comment rdf:datatype="&xsd;string"
