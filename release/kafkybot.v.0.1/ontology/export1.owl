    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/SpatialRelations.owl#descriptive-origin-of -->

    <owl:ObjectProperty rdf:about="&SpatialRelations;descriptive-origin-of">
        <rdfs:range rdf:resource="&DOLCE-Lite;endurant"/>
