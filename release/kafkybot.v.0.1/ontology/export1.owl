        <rdfs:domain rdf:resource="&DOLCE-Lite;perdurant"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;perdurant"/>
        <rdfs:subPropertyOf rdf:resource="&TemporalRelations;temporally-included-in"/>
