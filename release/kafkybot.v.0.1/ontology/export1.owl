        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;exact-location-of"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;non-physical-endurant"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;space-region"/>
