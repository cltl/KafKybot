    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY Systems "http://www.loa-cnr.it/ontologies/Systems.owl#" >
    <!ENTITY DOLCE-Lite "http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#" >
    <!ENTITY Collections "http://www.loa-cnr.it/ontologies/Collections.owl#" >
    <!ENTITY Collectives "http://www.loa-cnr.it/ontologies/Collectives.owl#" >
    <!ENTITY SocialUnits "http://www.loa-cnr.it/ontologies/SocialUnits.owl#" >
    <!ENTITY ExtendedDnS "http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#" >
    <!ENTITY InformationObjects "http://www.loa-cnr.it/ontologies/InformationObjects.owl#" >
]>


<rdf:RDF xmlns="http://www.loa-cnr.it/ontologies/SocialUnits.owl#"
     xml:base="http://www.loa-cnr.it/ontologies/SocialUnits.owl"
     xmlns:DOLCE-Lite="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
     xmlns:Collections="http://www.loa-cnr.it/ontologies/Collections.owl#"
     xmlns:SocialUnits="http://www.loa-cnr.it/ontologies/SocialUnits.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:Systems="http://www.loa-cnr.it/ontologies/Systems.owl#"
     xmlns:ExtendedDnS="http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#"
     xmlns:Collectives="http://www.loa-cnr.it/ontologies/Collectives.owl#"
     xmlns:InformationObjects="http://www.loa-cnr.it/ontologies/InformationObjects.owl#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo rdf:datatype="&xsd;string">397</owl:versionInfo>
        <rdfs:comment rdf:datatype="&xsd;string"
            >An ontology of collections, collectives, social units, and systems, plugins to DnS. These are placeholders for a more ambitious ontology of basic entities from the social world. OWL engineering by Aldo Gangemi.</rdfs:comment>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Imports: Information</rdfs:comment>
        <owl:imports rdf:resource="http://www.loa-cnr.it/ontologies/InformationObjects.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generic-location -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;generic-location"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generically-dependent-on -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;generically-dependent-on"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#mediated-relation -->

