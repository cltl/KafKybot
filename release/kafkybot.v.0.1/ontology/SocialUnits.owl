    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#specifically-constantly-dependent-on -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;specifically-constantly-dependent-on"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#acted-by -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;acted-by"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#characterized-by -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;characterized-by"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#characterizes -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;characterizes"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#covered-by -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;covered-by"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#d-uses -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;d-uses"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#hypostasis-of -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;hypostasis-of"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#involved-in -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;involved-in"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#involves -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;involves"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#member -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;member"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#member-of -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;member-of"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#requisite-for -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;requisite-for"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#satisfied-by -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;satisfied-by"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#satisfies -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;satisfies"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#unified-by -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;unified-by"/>
    


    <!-- http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#value-for -->

    <owl:ObjectProperty rdf:about="&ExtendedDnS;value-for"/>
    


    <!-- http://www.loa-cnr.it/ontologies/SocialUnits.owl#enforced-by -->

    <owl:ObjectProperty rdf:about="#enforced-by">
        <rdfs:subPropertyOf rdf:resource="&ExtendedDnS;involves"/>
        <rdfs:domain rdf:resource="&ExtendedDnS;regulation"/>
        <rdfs:range rdf:resource="#institution"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/SocialUnits.owl#enforces -->

    <owl:ObjectProperty rdf:about="#enforces">
        <rdfs:comment rdf:datatype="&xsd;string"
            >An institution enforces a regulation by playing a role within the part of that regulation that describes its enforcement.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ExtendedDnS;involved-in"/>
        <rdfs:range rdf:resource="&ExtendedDnS;regulation"/>
        <owl:inverseOf rdf:resource="#enforced-by"/>
        <rdfs:domain rdf:resource="#institution"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/SocialUnits.owl#ruled-by -->

    <owl:ObjectProperty rdf:about="#ruled-by">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation-i"/>
        <rdfs:range rdf:resource="#socially-constructed-person"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ExtendedDnS;figure"/>
                    <rdf:Description rdf:about="&ExtendedDnS;role"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/SocialUnits.owl#rules -->

    <owl:ObjectProperty rdf:about="#rules">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A control relation between socially constructed persons and either roles or figures. It requires that the socially constructed person is explicitly enabled to control in a description that d-uses both the socially constructed person and the controlled roles or figures.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation"/>
        <owl:inverseOf rdf:resource="#ruled-by"/>
        <rdfs:domain rdf:resource="#socially-constructed-person"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ExtendedDnS;figure"/>
                    <rdf:Description rdf:about="&ExtendedDnS;role"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


