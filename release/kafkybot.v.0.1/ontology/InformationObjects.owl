    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#broader-than -->

    <owl:ObjectProperty rdf:about="#broader-than">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The proper part relation between subjects.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;proper-part"/>
        <rdfs:range rdf:resource="#subject"/>
        <rdfs:domain rdf:resource="#subject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#identified-by -->

    <owl:ObjectProperty rdf:about="#identified-by">
        <rdfs:subPropertyOf rdf:resource="&ExtendedDnS;aboutness-of"/>
        <owl:inverseOf rdf:resource="#identifies"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#identifies -->

    <owl:ObjectProperty rdf:about="#identifies">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Being about an antity with the main purpose of conventionally naming that entity. Typically, proper nouns identify entities.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ExtendedDnS;about"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#mapped-to -->

    <owl:ObjectProperty rdf:about="#mapped-to">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Any entity e1 is mapped to any other entity e2 when one of the concepts that classify e1, e.g. c1, is contextually augmented by another concept c2 from either the same description as c1 (metonymy), or from another description (metaphor).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;particular"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;particular"/>
        <owl:inverseOf rdf:resource="#mapping-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#mapping-of -->

    <owl:ObjectProperty rdf:about="#mapping-of">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation-i"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;particular"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;particular"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#matonymical-mapping-of -->

    <owl:ObjectProperty rdf:about="#matonymical-mapping-of">
        <rdfs:subPropertyOf rdf:resource="#mapping-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#metaphorical-mapping.of -->

    <owl:ObjectProperty rdf:about="#metaphorical-mapping.of">
        <rdfs:subPropertyOf rdf:resource="#mapping-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#metaphorically-mapped-to -->

    <owl:ObjectProperty rdf:about="#metaphorically-mapped-to">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Any entity e1 is metaphorically mapped to any other entity e2 when one of the concepts that classify e1, e.g. c1, is contextually augmented by another concept c2 from another description.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#mapped-to"/>
        <owl:inverseOf rdf:resource="#metaphorical-mapping.of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#metonymically-mapped-to -->

    <owl:ObjectProperty rdf:about="#metonymically-mapped-to">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Any entity e1 is metonymically mapped to any other entity e2 when one of the concepts that classify e1, e.g. c1, is contextually augmented by another concept c2 from the same description as c1.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#mapped-to"/>
        <owl:inverseOf rdf:resource="#matonymical-mapping-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#narrower-than -->

    <owl:ObjectProperty rdf:about="#narrower-than">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;immediate-relation-i"/>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;part-of"/>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;proper-part-of"/>
        <owl:inverseOf rdf:resource="#broader-than"/>
        <rdfs:range rdf:resource="#subject"/>
        <rdfs:domain rdf:resource="#subject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#prototype -->

    <owl:ObjectProperty rdf:about="#prototype">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation between an entity (playing the role of example, sample, prototype, master, etc.), and another that has all the properties of the first (or a given set of them), except space-time.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;immediate-relation"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;particular"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;particular"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#prototype-for -->

    <owl:ObjectProperty rdf:about="#prototype-for">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;immediate-relation-i"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;particular"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;particular"/>
        <owl:inverseOf rdf:resource="#prototype"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#q-represented-by -->

    <owl:ObjectProperty rdf:about="#q-represented-by">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation-i"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;region"/>
        <rdfs:range rdf:resource="&ExtendedDnS;information-object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#q-represents -->

    <owl:ObjectProperty rdf:about="#q-represents">
        <rdfs:comment rdf:datatype="&xsd;string"
            >This relation supports the representation of conceptual regions by information objects. It is defined as a composed relation: an information object is expressed according to an information encoding description that maps a quality space. In other words, this means that a representation of conceptual regions within quality spaces requires an explicit conceptualization of the dimensions operating in the quality space. In still other words, a quality space can be mapped to a theory, which can be reified as a special kind of &#39;information encoding description&#39;.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;region"/>
        <rdfs:domain rdf:resource="&ExtendedDnS;information-object"/>
        <owl:inverseOf rdf:resource="#q-represented-by"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#referred-by -->

    <owl:ObjectProperty rdf:about="#referred-by">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation-i"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;particular"/>
        <rdfs:range rdf:resource="&ExtendedDnS;agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/InformationObjects.owl#refers-to -->

    <owl:ObjectProperty rdf:about="#refers-to">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Referring to something is assumed here under a &#39;negotiated reference&#39; approach, i.e. agents refer to entities by conceiving a description appropriate to context.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;mediated-relation"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;particular"/>
        <rdfs:domain rdf:resource="&ExtendedDnS;agent"/>
        <owl:inverseOf rdf:resource="#referred-by"/>
    </owl:ObjectProperty>
    


